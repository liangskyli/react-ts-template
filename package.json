{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "一套基于react、ts、vite的项目模板,帮助快速搭建react项目",
  "keywords": [
    "react ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/liangskyli/react-ts-template",
  "bugs": "https://github.com/liangskyli/react-ts-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/liangskyli/react-ts-template"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "scripts": {
    "build:dev": "pnpm vite build -m dev",
    "build:pro": "pnpm vite build -m pro",
    "build:test": "pnpm vite build -m test",
    "coverage": "vitest run --coverage",
    "deps:check": "pnpm outdated -r",
    "dev": "vite -m dev",
    "dev:mock": "vite -m dev-mock",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "dev:pro": "vite -m pro",
    "dev:test": "vite -m test",
    "preinstall": "npx only-allow pnpm",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less}\" --fix",
    "mock-gen": "pnpm http-mock-gen -c mock-gen.config.ts",
    "prepare": "husky",
    "preview:dev": "pnpm vite preview -m dev",
    "preview:pro": "pnpm vite preview -m pro",
    "preview:test": "pnpm vite preview -m test",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "typecheck": "tsc --noEmit",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.9",
    "@liangskyli/axios-request": "^0.2.2",
    "axios": "^1.13.2",
    "immer": "^11.1.3",
    "lru-cache": "^11.2.4",
    "optics-ts": "^2.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-pdf": "^10.3.0",
    "react-router": "^7.12.0",
    "react-virtualized": "^9.22.6",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^3.2.2",
    "typeit-react": "^2.7.8",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/prompt-cli": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@liangskyli/http-mock-gen": "^7.0.1",
    "@liangskyli/mock": "^7.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/react-virtualized": "^9.22.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-dev-inspector": "^2.0.1",
    "stylelint": "^17.0.0",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-less": "^4.0.1",
    "stylelint-order": "^7.0.1",
    "tailwindcss": "^3.4.19",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": "^10"
  }
}
