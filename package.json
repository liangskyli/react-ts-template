{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "一套基于react19、ts、vite6的项目模板,帮助快速搭建react项目",
  "keywords": [
    "react19 ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/liangskyli/react-ts-template",
  "bugs": "https://github.com/liangskyli/react-ts-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/liangskyli/react-ts-template"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "scripts": {
    "build:dev": "pnpm vite build -m dev",
    "build:pro": "pnpm vite build -m pro",
    "build:test": "pnpm vite build -m test",
    "coverage": "vitest run --coverage",
    "deps:check": "pnpm outdated -r",
    "dev": "vite -m dev",
    "dev:mock": "vite -m dev-mock",
    "dev:mock-server": "pnpm mock-server -c mock.config.ts",
    "dev:pro": "vite -m pro",
    "dev:test": "vite -m test",
    "preinstall": "npx only-allow pnpm",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,scss,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less,scss}\" --fix",
    "prepare": "husky",
    "preview:dev": "pnpm vite preview -m dev",
    "preview:pro": "pnpm vite preview -m pro",
    "preview:test": "pnpm vite preview -m test",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "typecheck": "tsc --noEmit",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.9",
    "@liangskyli/axios-request": "^0.2.2",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "immer": "^10.2.0",
    "lru-cache": "^11.2.2",
    "optics-ts": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-pdf": "^10.2.0",
    "react-router": "^7.9.6",
    "react-virtualized": "^9.22.6",
    "tailwind-merge": "^2.6.0",
    "typeit-react": "^2.7.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@eslint/js": "^9.39.1",
    "@liangskyli/mock": "^6.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-virtualized": "^9.22.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.10",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "less": "^4.4.2",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-dev-inspector": "^2.0.1",
    "sass": "^1.94.1",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": "^10"
  }
}
